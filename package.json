    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.1",
    "git-commit-msg-linter": "^3.2.8",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": ">=3.3.1 <4.4.0"
